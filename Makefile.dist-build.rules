clean:
	rm -rf $(DIST) $(BUILD) dist build

$(DIST):
	mkdir -p $(DIST)
	ln -sf $(DIST) dist

$(BUILD):
	mkdir -p $(BUILD)
	ln -sf $(BUILD) build

